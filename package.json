{
  "name": "klinikmat",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "postinstall": "prisma generate",
    "dev": "next dev",
    "dev:mail": "docker-compose up -d mailhog && npm run dev",
    "dev:mail:down": "docker-compose down",
    "prebuild": "node scripts/check-env.mjs",
    "build": "next build",
    "start": "next start",
    "lint": "next lint --max-warnings=0",
    "db:seed": "node -r dotenv/config node_modules/ts-node/dist/bin.js --compiler-options '{\"module\":\"CommonJS\"}' prisma/seed.ts",
    "seed:cases": "node -r dotenv/config node_modules/ts-node/dist/bin.js --compiler-options '{\"module\":\"CommonJS\"}' scripts/seed-cases.ts",
    "typecheck": "tsc --noEmit",
    "typecheck:paths": "tsc --noEmit --traceResolution",
    "db:push": "node -r dotenv/config node_modules/.bin/prisma db push",
    "test:env": "node -r dotenv/config -e 'console.log(`DATABASE_URL is: ${process.env.DATABASE_URL}`)'"
  },
  "dependencies": {
    "@clerk/localizations": "^3.28.1",
    "@clerk/nextjs": "^6.35.2",
    "@heroicons/react": "^2.2.0",
    "@prisma/client": "6.19.0",
    "clsx": "^2.1.1",
    "jose": "^6.1.1",
    "lucide-react": "^0.554.0",
    "next": "^14.2.33",
    "prisma": "6.19.0",
    "react": "^18",
    "react-dom": "^18",
    "svix": "^1.81.0"
  },
  "devDependencies": {
    "@next/swc-darwin-x64": "^16.0.4",
    "@tailwindcss/container-queries": "^0.1.1",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "autoprefixer": "^10.4.21",
    "dotenv": "^17.2.3",
    "dotenv-cli": "^7.4.4",
    "eslint": "^8",
    "eslint-config-next": "14.2.3",
    "json5": "^2.2.3",
    "postcss": "^8",
    "tailwindcss": "^3.4.1",
    "tailwindcss-animate": "^1.0.7",
    "ts-node": "^10.9.2",
    "typescript": "^5"
  },
  "overrides": {
    "@auth/core": "0.34.1"
  }
}
